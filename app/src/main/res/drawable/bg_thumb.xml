<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <solid android:color="#62FD7B" />
            <size
                android:width="12dp"
                android:height="12dp" />
            <stroke
                android:width="0.1dp"
                android:color="@color/black" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <solid android:color="#ffffff" />
            <size
                android:width="12dp"
                android:height="12dp" />
            <stroke
                android:width="0.1dp"
                android:color="@color/black" />

        </shape>
    </item>


</layer-list>